      <dependency id="NetOfficeFw.Core" version="[$version$]" />
    </dependencies>
  </metadata>
  <files>
    <!-- net40 -->
    <file src="..\libs\NET 4.0\Assemblies\Any CPU\WordApi.dll" target="lib\net40" />
    <file src="..\libs\NET 4.0\Assemblies\Any CPU\WordApi.pdb" target="lib\net40" />
    <file src="..\libs\NET 4.0\Assemblies\Any CPU\WordApi.xml" target="lib\net40" />

    <!-- net45 -->
