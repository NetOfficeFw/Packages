  <metadata>
    <id>NetOfficeFw.Word</id>
    <version>$version$</version>
    <title>NetOfficeFw.Word</title>
    <authors>Sebastian Lange, Jozef Izso</authors>
    <owners>Jozef Izso</owners>
    <licenseUrl>https://raw.githubusercontent.com/NetOfficeFw/NetOffice/master/LICENSE.txt</licenseUrl>
